CREATE TABLE "FACTURACION" (
"FACN_ID" INTEGER NOT NULL primary key
 	GENERATED ALWAYS AS IDENTITY
        (START WITH 1, INCREMENT BY 1),
-- Para que la relacion con USUARIOS sea OneToOne
"USER_ID" INTEGER NOT NULL constraint col_uniq UNIQUE, 
"FAC_NAME" VARCHAR(250) default NULL, 
"FAC_DIRECCION" VARCHAR(250) DEFAULT NULL,
"FAC_POBLACION" CHAR(125) DEFAULT NULL,
"FAC_PROVINCIA" CHAR(50) DEFAULT NULL,
"FAC_CODPOSTAL" CHAR (5) DEFAULT NULL,
CONSTRAINT FAC_FK
	FOREIGN KEY (USER_ID)
	REFERENCES USUARIOS (USER_ID)

);
