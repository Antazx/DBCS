CREATE TABLE "USUARIOS" (
"USER_ID" INTEGER NOT NULL primary key
        GENERATED ALWAYS AS IDENTITY
        (START WITH 1, INCREMENT BY 1), 
"USER_NAME" VARCHAR(20) default NULL, 
"USER_PASSWD" VARCHAR(8) DEFAULT NULL
);

INSERT INTO USUARIOS (USER_NAME,USER_PASSWD) VALUES 
    ('Usuario1','1111'),
    ('Usuario2','2222'),
    ('Usuario3','3333');

CREATE TABLE "PRODUCTOS" (
"PROD_ID" INTEGER NOT NULL primary key
        GENERATED ALWAYS AS IDENTITY
        (START WITH 1, INCREMENT BY 1), 
"PROD_DESCRIPTION" VARCHAR(255) default NULL, 
"PROD_PRICE" REAL DEFAULT 0.0, 
"ITEM_ID" VARCHAR(15) DEFAULT NULL
);

INSERT INTO PRODUCTOS(PROD_DESCRIPTION,PROD_PRICE,ITEM_ID) VALUES 
    ('Producto1: componente11, componente12',11.88,'78jK889'),
    ('Producto2: componente21, componente22',234.56,'AKJN89II'),
    ('Producto3: componente31, componente32',34.88,'778899YY88'),
    ('Producto4: componente41, componente42, componente43',567.03,'8UUgY78ii'),
    ('Producto5: componente51',125.34,'Ui444yy6H-1-8');

CREATE TABLE "FACTURACION" (
"FACN_ID" INTEGER NOT NULL primary key
 	GENERATED ALWAYS AS IDENTITY
        (START WITH 1, INCREMENT BY 1),
-- Para que la relacion con USUARIOS sea OneToOne
"USER_ID" INTEGER NOT NULL constraint col_uniq UNIQUE, 
"FAC_NAME" VARCHAR(250) default NULL, 
"FAC_DIRECCION" VARCHAR(250) DEFAULT NULL,
"FAC_POBLACION" CHAR(125) DEFAULT NULL,
"FAC_PROVINCIA" CHAR(50) DEFAULT NULL,
"FAC_CODPOSTAL" CHAR (5) DEFAULT NULL,
CONSTRAINT FAC_FK
	FOREIGN KEY (USER_ID)
	REFERENCES USUARIOS (USER_ID)
);

INSERT INTO FACTURACION (USER_ID,FAC_NAME,FAC_ADDRESS,FAC_CITY,FAC_STATE,
FAC_ZIP) VALUES 
    (1,'Nombre1 Apellido11 Apellido12','Calle Usuario1','Poblacion1','Provincia1','11111'),
    (3,'Nombre3 Apellido31 Apellido32','Calle Usuario3','Poblacion3','Provincia3','33333'),
    (2,'Nombre2 Apellido21 Apellido22','Calle Usuario2','Poblacion2','Provincia2','22222');

